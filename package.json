  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/stories/**/*",
      "!src/**/index.js",
      "!src/**/context.js",
      "!src/**/log.js",
      "!src/modules/TerraFrontProvider.js"
